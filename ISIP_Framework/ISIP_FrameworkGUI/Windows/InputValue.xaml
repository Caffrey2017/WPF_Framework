<Window x:Class="ISIP_FrameworkGUI.Windows.InputValue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISIP_FrameworkGUI.Windows"
        mc:Ignorable="d"
        Title="Value" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Value:" HorizontalAlignment="Left" VerticalAlignment="Top" Width="63" Margin="27,111,0,0"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="95,111,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="OK" HorizontalAlignment="Left" Margin="100,217,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Separator HorizontalAlignment="Left" Height="270" Margin="95,0,0,0" VerticalAlignment="Top" Width="0"/>
    </Grid>
</Window>
